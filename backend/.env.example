# Quote Scraper Configuration
# Copy this file to .env and fill in your credentials

# Quotes.toscrape.com login credentials (optional)
# If not provided, the scraper will work without login
QUOTES_USERNAME=your_username_here
QUOTES_PASSWORD=your_password_here

# Login URL (default: https://quotes.toscrape.com/login)
QUOTES_LOGIN_URL=https://quotes.toscrape.com/login

# Server Configuration
PORT=5000

# Scraper Configuration
MAX_PAGES=10
MAX_CONCURRENT_TABS=10
TIMEOUT=30000
